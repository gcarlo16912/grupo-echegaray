    // Header
    // Banner principal
    header.header-main.banner-slider-image
        #carouselExample2.carousel.slide(data-ride='carousel')
            .carousel-inner
                .carousel-item.active
                    img.d-block.w-100(src= url+'img/banner_header_01.jpg' alt='Banner 01')
                .carousel-item
                    img.d-block.w-100(src= url+'img/banner_header_02.jpg' alt='Banner 02')
