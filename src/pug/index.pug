    // Seccion Cliente
    section#cliente.section-main
        .container
            .row
                .col.col-lg-12
                    h2.text-center.mb-5.section-title Cliente
                each cliente in clientes
                    .col.col-lg-3.mb-0.mb-lg-4
                        .container-cliente
                            .img-cliente
                                img(src= `img/${cliente.img}`, alt= cliente.text, title= cliente.text)

    // Seccion Banner Video
    section.section-main.bg-banner-video.content-section-banner-video
